{
  "name": "evoque-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/evoque-web/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project evoque-web --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/evoque-web"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/evoque-web"
        },
        "production": {}
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "evoque-web:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "evoque-web:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "evoque-web:build:production",
          "dev": false
        }
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "evoque-web:build",
        "dev": false
      },
      "configurations": {
        "development": {
          "buildTarget": "evoque-web:build:development",
          "dev": false
        },
        "production": {
          "buildTarget": "evoque-web:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "evoque-web:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/evoque-web/jest.config.cts",
        "passWithNoTests": true
      }
    }
  }
}
