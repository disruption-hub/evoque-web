{
  "name": "websocket",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/websocket/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/websocket",
        "webpackConfig": "apps/websocket/webpack.config.js"
      }
    },
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/websocket/package.json",
        "{workspaceRoot}/dist/apps/websocket/package-lock.json"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": ["{workspaceRoot}/dist/apps/websocket/workspace_modules"],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "websocket:build",
        "watch": false,
        "inspect": false
      },
      "configurations": {
        "development": {
          "buildTarget": "websocket:build:development",
          "watch": false
        },
        "production": {
          "buildTarget": "websocket:build:production",
          "watch": false
        }
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}

